<h3 class='contact-title'>UNDP <%= unit %> Office</h3>
<ul class='contact-info'>
  <% if (website && website.length > 1) { %>
  <li>
    <span class='label'>Website</span>
    <a href='<%= website %>'><%= website %></a>
  </li>
  <% } else if (email && email.length > 1) { %>
  <li>
    <span class='label'>Email</span>
    <a href='mailto:<%= email %>'><%= email %></a>
  </li>
  <% } else if (twitter.length) { %>
  <li>
    <span class='label'>Twitter</span>
    <a href='https://twitter.com/<%= twitter %>'>@<%= twitter %></a>
  </li>
  <% } else if (flickr.length){ %>
  <li>
    <span class='label'>Flickr</span>
    <a href='https://flickr.com/photos/<%= flickr %>'><%= flickr %></a>
  </li>
  <% } else if (facebook.length){ %>
  <li>
    <span class='label'>Facebook</span>
    <a href='https://facebook.com/<%= facebook %>'><%= facebook %></a>
  </li>
  <% } %>
</ul>