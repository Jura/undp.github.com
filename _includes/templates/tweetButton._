<a href='https://twitter.com/share' class='twitter-share-button'
	data-count='none' 
	data-url='<%= url %>' 
	data-hashtags='<%= hashtags %>' 
	data-text='<%= text %>' 
	data-via='<%= via %>'
></a>
<a href='https://twitter.com/<%= twitter %>' class='twitter-follow-button'
data-show-count='true'>
	Follow @<%= twitter %>
</a>